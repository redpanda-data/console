{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9090",
  "homepage": ".",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "rsbuild dev",
    "start2": "REACT_APP_ENABLED_FEATURES=SINGLE_SIGN_ON,REASSIGN_PARTITIONS rsbuild dev",
    "preview": "rsbuild preview",
    "build": "NODE_OPTIONS=--max_old_space_size=16384 rsbuild build",
    "build-local-test": "REACT_APP_BUSINESS=true REACT_APP_CONSOLE_GIT_SHA=abc123 REACT_APP_CONSOLE_GIT_REF=local REACT_APP_BUILD_TIMESTAMP=32503680000 REACT_APP_DEV_HINT=true NODE_OPTIONS=--max_old_space_size=16384 rsbuild build",
    "install:chromium": "bunx playwright install chromium",
    "e2e-test": "playwright test tests/console/ -c playwright.config.ts",
    "e2e-test-enterprise": "playwright test tests/console-enterprise/ -c playwright.enterprise.config.ts",
    "e2e-test:ui": "playwright test tests/console/ -c playwright.config.ts --ui",
    "test": "TZ=GMT vitest run",
    "test:coverage": "TZ=GMT vitest run --coverage",
    "test:ui": "TZ=GMT vitest --ui",
    "test:watch": "TZ=GMT vitest --watch --ui",
    "type:check": "tsc --noEmit --incremental --p tsconfig.base.json",
    "lint": "biome check src/* --write --unsafe",
    "linter": "biome ci src/*",
    "lint:check": "biome ci src/*",
    "format:check": "biome format src/*",
    "format": "biome format src/* --write",
    "load-git-hooks": "cd .. && git config core.hooksPath \"./.git-hooks\"",
    "analyze": "RSDOCTOR=true npm run build"
  },
  "overrides": {
    "elliptic": "^6.6.0"
  },

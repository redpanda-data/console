{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9090",
  "homepage": ".",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "start": "rsbuild dev",
    "start2": "REACT_APP_ENABLED_FEATURES=SINGLE_SIGN_ON,REASSIGN_PARTITIONS rsbuild dev",
    "preview": "rsbuild preview",
    "build": "NODE_OPTIONS=--max_old_space_size=16384 rsbuild build",
    "build-local-test": "REACT_APP_BUSINESS=true REACT_APP_CONSOLE_GIT_SHA=abc123 REACT_APP_CONSOLE_GIT_REF=local REACT_APP_BUILD_TIMESTAMP=32503680000 REACT_APP_DEV_HINT=true NODE_OPTIONS=--max_old_space_size=16384 rsbuild build",
    "install:browsers": "npx playwright install --with-deps",
    "e2e-test": "playwright test tests/console/ -c playwright.config.ts",
    "e2e-test-enterprise": "playwright test tests/console-enterprise/ -c playwright.enterprise.config.ts",
    "test": "TZ=GMT vitest run",
    "test:coverage": "TZ=GMT vitest run --coverage",
    "test:ui": "TZ=GMT vitest --ui",
    "test:watch": "TZ=GMT vitest --watch --ui",
    "type:check": "tsc --noEmit --incremental --p tsconfig.base.json",
    "lint": "biome check src/* --write --unsafe",
    "linter": "biome ci src/*",
    "lint:check": "biome ci src/*",
    "format:check": "biome format src/*",
    "format": "biome format src/* --write",
    "fetch-monaco": "node scripts/fetch-monaco.mjs",
    "load-git-hooks": "cd .. && git config core.hooksPath \"./.git-hooks\"",
    "prepare": "npm run fetch-monaco",
    "analyze": "RSDOCTOR=true npm run build"
  },
  "overrides": {
    "elliptic": "^6.6.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1",
    "@chakra-ui/react-use-disclosure": "^2.1",
    "@connectrpc/connect": "^1.1.2",
    "@connectrpc/connect-query": "^1.4.2",
    "@connectrpc/connect-web": "^1.1.2",
    "@heroicons/react": "^1.0.6",
    "@monaco-editor/react": "^4.6.0",
    "@primer/octicons-react": "^17",
    "@redpanda-data/ui": "^4.2.0",
    "@tanstack/react-query": "^5.61.5",
    "array-move": "^4",
    "framer-motion": "^7",
    "js-base64": "^3.7.7",
    "mobx": "^6.10",
    "mobx-react": "^7.6",
    "moment": "^2.29.4",
    "monaco-editor": "^0.52.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "monaco-yaml": "^5.2.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pretty-bytes": "^5.6.0",
    "pretty-ms": "^7.0.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.3",
    "react-highlight-words": "^0.18.0",
    "react-icons": "^4.10.1",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-prism": "^1.3.6",
    "stacktrace-js": "^2.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@module-federation/enhanced": "^0.7.7",
    "@module-federation/runtime": "^0.7.7",
    "@playwright/test": "^1.49.0",
    "@rsbuild/core": "1.1.6",
    "@rsbuild/plugin-react": "1.0.7",
    "@rsbuild/plugin-sass": "1.1.1",
    "@rsbuild/plugin-svgr": "1.0.5",
    "@rsdoctor/rspack-plugin": "^0.4.9",
    "@rspack/cli": "1.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^18",
    "@types/react": "^18.0.18",
    "@types/react-beautiful-dnd": "^13.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-router-dom": "^5.3",
    "@types/react-syntax-highlighter": "^15",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.6",
    "node-fetch": "^3.3.0",
    "tar-stream": "^2.2.0",
    "ts-node": "^10.9",
    "typescript": "^5.7.2",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "resolutions": {
    "react-error-overlay": "6.0.11",
    "immer": "9.0.7"
  }
}
